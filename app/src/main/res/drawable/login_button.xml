<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_pressed="false"><shape>
        <stroke android:width="1dp" android:color="#000000" />

        <solid android:color="@android:color/transparent" />

        <corners
            android:radius="5dp"/>
    </shape></item>


    <item
        android:state_pressed="true"><shape>
        <stroke android:width="1dp" android:color="#eaf4ea" />

        <solid android:color="@android:color/transparent" />

        <corners
            android:radius="5dp"/>
    </shape></item>

</selector>